{
  "name": "@devex/elegant-swagger-codegen",
  "version": "0.5.0",
  "license": "MIT",
  "exports": "./main.ts",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "start": "deno run --allow-env --allow-read --allow-net --allow-write main.ts",
    "generate": "deno run --allow-env --allow-read --allow-net --allow-write main.ts generate",
    "validate": "deno run --allow-env --allow-read --allow-net main.ts validate",
    "info": "deno run --allow-env --allow-read --allow-net main.ts info",
    "swagger": "swagger-typescript-api generate --path ./swagger.json --output ./app/generated --no-client --name swagger.ts"
  },
  "imports": {
    "citty": "npm:citty@0.1.6",
    "consola": "npm:consola@3.4.2",
    "ofetch": "npm:ofetch@1.4.1",
    "openapi-types": "npm:openapi-types@12.1.3",
    "scule": "npm:scule@1.3.0",
    "swagger-typescript-api": "npm:swagger-typescript-api@13.2.8"
  }
}
